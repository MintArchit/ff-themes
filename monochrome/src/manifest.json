    "properties": {
      "additional_backgrounds_alignment": [
        "center center",
        "right center"
      ],
      "additional_backgrounds_tiling": [
        "repeat",
        "no-repeat"
      ],
      "color_scheme": "dark",
      "content_color_scheme": "dark"
    }
  },
  "browser_specific_settings":{
    "gecko": {
      "id": "template@mintarchit.com",
      "strict_min_version": "100.0"
    }
  },
  // currently must be 2
  "manifest_version": 2
}
