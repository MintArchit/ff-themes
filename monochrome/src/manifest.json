{
  // keep name short, max 45 chars
  "name": "Monochrome Dark",
  // short_name max 12 chars
  "short_name": "monochrome",
  "version": "3.8",
  //"author": "MintArchit",
  "description": "A grayscale ff theme with all color keys used",
  // specified dev key replaces author key
  "developer": {
    "name": "MintArchit",
    "url": "https://mintarchit.com"
  },
  "icons": {
    "32": "img/icon32.svg"
  },
  "theme":{
    "images": {
      //"theme_frame": "",
      "additional_backgrounds": [
        "img/bg-037.svg",
        "img/0.png"
      ]
    },
    "colors": {
      "bookmark_text": "rgb(255, 255, 255)",
      "button_background_active": "rgba(74, 74, 74, 0.7)",
      "button_background_hover": "rgba(225, 226, 225, 0.7)",
      "icons": "rgb(226, 226, 226)",
      "icons_attention": "rgb(164, 164, 164)",
      "frame": "rgba(56, 56, 56, 0.3)",
      "frame_inactive": "rgba(0, 0, 0, 0.5)",
      "ntp_background": "rgb(66, 66, 66)",
      "ntp_text": "rgb(255, 255, 255)",
      "popup": "rgba(23, 23, 23, 0.7)",
      "popup_border": "rgb(0, 0, 0)",
      "popup_highlight": "rgb(5, 5, 5)",
      "popup_highlight_text": "rgb(255, 255, 255)",
      "popup_text": "rgb(255, 255, 255)",
      "sidebar": "rgba(75, 75, 75, 1)",
      "sidebar_border": "rgb(5, 5, 5)",
      "sidebar_highlight": "rgb(231, 231, 231)",
      "sidebar_highlight_text": "rgb(0, 0, 0)",
      "sidebar_text": "rgb(255, 255, 255)",
      "tab_background_text": "rgb(193, 193, 193)",
      "tab_line": "rgb(255, 255, 255)",
      "tab_loading": "rgb(138, 138, 138)",
      "tab_selected": "rgba(233, 233, 233, 1)",
      "tab_text": "rgb(0, 0, 0)",
      "toolbar": "rgba(31, 34, 61, 0)",
