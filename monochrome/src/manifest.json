      "toolbar_field_text_focus": "rgb(255, 255, 255)",
      "toolbar_text": "rgb(255, 255, 255)",
      "toolbar_top_separator": "rgb(255, 255, 255)",
      "toolbar_vertical_separator": "rgb(255, 255, 255)"
